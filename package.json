    "start:pm2": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts pm2 start nodemon --name ecommerce",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "build:tsc": "tsc",
    "build:components": "webpack --config dist/payload/webpack/components.config.js",
    "build": "yarn copyfiles && yarn build:tsc && yarn build:components",
    "build:watch": "nodemon --watch 'src/**' --ext 'ts,tsx' --exec \"yarn build:tsc\"",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts ts-node -T  ./src/payload/bin/build.ts",
    "dev:generate-types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts ts-node -T ./src/generateTypes.ts",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js"
